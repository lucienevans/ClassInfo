<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <gradient android:angle="270" android:endColor="#8ACDE6" android:startColor="#8ACDE6" />

            <stroke android:width="1dp" android:color="#8ACDE6" />


            <padding android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp" />
        </shape></item>
    <item android:state_focused="true"><shape>
            <gradient android:angle="270" android:endColor="#8ACDE6" android:startColor="#8ACDE6" />

            <stroke android:width="1dp" android:color="#8ACDE6" />


            <padding android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp" />
        </shape></item>
    <item><shape>
            <gradient android:angle="270" android:endColor="#00ffffff" android:startColor="#00ffffff" />

            <stroke android:width="1dp" android:color="#00ffffff" />


            <padding android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp" />
        </shape></item>

</selector>